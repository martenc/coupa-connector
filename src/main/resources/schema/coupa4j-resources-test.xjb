<jaxb:bindings version="1.0"
  xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
  jaxb:extensionBindingPrefixes="xjc">
  <jaxb:bindings schemaLocation="coupa4j-resources-test.xsd" node="/xs:schema">
      <jaxb:globalBindings fixedAttributeAsConstantProperty="true">
         <xjc:simple />
         <jaxb:javaType name="java.util.Calendar" xmlType="xs:dateTime"
               parseMethod="javax.xml.bind.DatatypeConverter.parseDateTime"
               printMethod="com.coupa.transforms.XMLDateAdapter.printDate"/>
         <jaxb:javaType name="java.util.Calendar" xmlType="xs:date"
               parseMethod="com.coupa.transforms.XMLDateAdapter.parseDate"
               printMethod="com.coupa.transforms.XMLDateAdapter.printDate"/>
      </jaxb:globalBindings>
      <jaxb:bindings node="//xs:complexType[@name='resources']/xs:sequence/xs:choice/*[1]">
          <jaxb:property name='resources'/>
      </jaxb:bindings>
   </jaxb:bindings>
</jaxb:bindings>